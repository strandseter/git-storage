{
  "name": "gitstore",
  "version": "0.1.0",
  "description": "A lightweight TypeScript library to manage and commit data directly to a Git repository, enabling simple, Git-based storage solutions for small websites and static projects.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,json,md}\""
  },
  "keywords": [
    "git",
    "storage",
    "typescript",
    "database",
    "gitstore",
    "light",
    "simple"
  ],
  "author": "Anders Bj√∏rnstad Strandseter",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/node": "^22.15.17",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  }
}
